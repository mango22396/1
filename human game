				this.Msg_Pertinence(netHost, "huisu 2 :设置为 回溯式读点", "");
				this.Msg_Pertinence(netHost, "huisu 3 :设置为 有惯性读点", "");
			}
			netHost.players[0].human.attribute.huisu = num;
			if (num == 0)
			{
				this.Msg_Pertinence(netHost, "已将自己的读点方式设置为 房间默认读点", "");
				return;
			}
			if (num == 1)
			{
				this.Msg_Pertinence(netHost, "已将自己的读点方式设置为 默认无惯性读点", "");
				return;
			}
