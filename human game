			{
				this.Update_Lobby();
			}
			if (this.Minute())
			{
				foreach (Human human3 in Human.all)
				{
					human3.attribute.SpokenCount = 0;
				}
			}
			if (this.preUpdate != null)
			{
				this.preUpdate();
			}
			foreach (Human human4 in Human.all)
			{
				if (human4.state == HumanState.Spawning)
				{
					break;
				}
				if (Host_Func.AllFly || human4.attribute.Fly)
				{
					if (human4.controls.walkSpeed > 0f)
					{
						human4.onGround = true;
						human4.state = HumanState.Walk;
					}
					else
					{
						human4.onGround = true;
						human4.state = HumanState.Idle;
					}
					if (human4.controls.jump)
					{
						human4.state = HumanState.Jump;
						human4.onGround = true;
						human4.jump = true;
					}
				}
				if (human4.attribute.Jump_Height > 0f && human4.state != HumanState.Idle && human4.state != HumanState.Spawning && human4.controls.jump)
				{
					this.GaoTiao(human4);
				}
			}
		}

