			foreach (Rigidbody rigidbody in UnityEngine.Object.FindObjectsOfType(typeof(Rigidbody)))
			{
				if (Vector3.Distance(rigidbody.position, raycastHit.point) < 3f && rigidbody.GetComponentInParent<Human>() == null)
				{
					num++;
					rigidbody.AddForce(rigidbody.mass * ((rigidbody.transform.position - human.transform.position) * 10f + Vector3.up * 5f) * 16.5f / raycastHit.distance, ForceMode.Impulse);
				}
			}
			if (num > 0)
			{
				human.ragdoll.partLeftArm.rigidbody.AddForce(-100f * human.targetDirection, ForceMode.Impulse);
				human.ragdoll.partChest.rigidbody.AddForce(-60f * human.targetDirection, ForceMode.Impulse);
				Host_Func.Only_Message(human.player.host, "fire!", "");
			}
			Human human2 = (raycastHit.transform.GetComponentInParent<Human>() == null) ? raycastHit.transform.GetComponent<Human>() : raycastHit.transform.GetComponentInParent<Human>();
			if (human2 == null)
			{
				return;
			}
			foreach (Rigidbody rigidbody2 in human2.rigidbodies)
			{
				rigidbody2.AddForce(rigidbody2.mass * ((rigidbody2.transform.position - human.transform.position) * 10f + Vector3.up * 5f) * 16.5f / raycastHit.distance, ForceMode.Impulse);
			}
			human.ragdoll.partLeftArm.rigidbody.AddForce(-100f * human.targetDirection, ForceMode.Impulse);
			human.ragdoll.partChest.rigidbody.AddForce(-60f * human.targetDirection, ForceMode.Impulse);
			Host_Func.Only_Message(human.player.host, "fire!", "");
		}

