		// Token: 0x0600512F RID: 20783 RVA: 0x001537F0 File Offset: 0x001519F0
		public void Judge_Tp_update()
		{
			bool flag = false;
			bool @bool = ValueLab.GetBool("Tp_Update", false);
			foreach (Human human in Human.all)
			{
				if (human.attribute.greatPower)
				{
					flag = true;
					break;
				}
				if (human.attribute.cannon_able)
				{
					flag = true;
					break;
				}
				if (human.attribute.Xiobject)
				{
					flag = true;
					break;
				}
			}
			if (flag && !@bool)
			{
				this.preUpdate += this.Tp_Update;
				ValueLab.SetBool("Tp_Update", true);
			}
			if (!flag && @bool)
			{
				this.preUpdate -= this.Tp_Update;
				ValueLab.SetBool("Tp_Update", false);
			}
		}

