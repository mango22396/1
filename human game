		// Token: 0x06005148 RID: 20808 RVA: 0x00156200 File Offset: 0x00154400
		public void AddLocalPlayer(uint localCoopIndex, byte[] crc)
		{
			if (NetGame.isClient)
			{
				NetStream netStream = NetGame.BeginMessage(NetMsgId.AddPlayer);
				try
				{
					netStream.WriteNetId(localCoopIndex);
					netStream.Write(NetGame.instance.transport.getUserId((int)localCoopIndex));
					netStream.WriteArray(crc, 8);
					NetGame.instance.SendReliableToServer(netStream);
					return;
				}
				finally
				{
					if (netStream != null)
					{
						netStream = netStream.Release();
					}
				}
			}
			this.AddServerPlayer(NetGame.instance.local, true, NetGame.instance.transport.getUserId((int)localCoopIndex), localCoopIndex, crc, false);
		}

