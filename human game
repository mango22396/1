		// Token: 0x06005068 RID: 20584 RVA: 0x0013C804 File Offset: 0x0013AA04
		private void getSkill(Human human)
		{
			this.getSkill_2(human);
			if (!this.alwayseFalse)
			{
				return;
			}
			int num;
			if (human.attribute.isHero)
			{
				if (Base.Random(1, 6) < 2)
				{
					num = this.Bug_Skill[Base.Random(0, this.Bug_Skill.Length - 1)];
				}
				else
				{
					num = Base.Random(0, this.skill_name.Length - 1);
				}
			}
			else if (Base.Random(1, 6) < 4)
			{
				if (human.attribute.Infected_Person)
				{
					num = this.Control_Skill[Base.Random(0, this.Control_Skill.Length - 1)];
				}
				else
				{
					num = this.WeiYi_Skil[Base.Random(0, this.WeiYi_Skil.Length - 1)];
				}
			}
			else
			{
				num = Base.Random(0, this.skill_name.Length - 1);
			}
			for (int i = 0; i < this.Once_Skill.Length; i++)
			{
				if (num.Equals(this.Once_Skill[i]))
				{
					human.attribute.skill_once = true;
				}
			}
			human.attribute.skill = num;
			human.attribute.skill_save = num;
			human.attribute.skill_own = true;
			if (human.attribute.skill == 3)
			{
				human.attribute.cooldown_deadline = 8;
			}
			else if (human.attribute.skill != 12 && human.attribute.skill != 17)
			{
				human.attribute.cooldown_deadline = 5;
			}
			else
			{
				human.attribute.cooldown_deadline = 30;
			}
			if (human.attribute.skill != 2 && human.attribute.skill != 6)
			{
				if (human.attribute.skill == 5)
				{
					human.attribute.skill_time_deadline = 300;
				}
				else if (human.attribute.skill == 14)
				{
					human.attribute.skill_time_deadline = 90;
				}
				else
				{
					human.attribute.skill_time_deadline = 60;
				}
			}
			else
			{
				human.attribute.skill_time_deadline = 90;
			}
			this.Custom_Message(0u, string.Format("<size=18><#{1}>{0}</color></size><size=18><#FFFFFF>获得技能: {2}，按E键使用</color></size>", human.player.host.name, this.infect_color[human.attribute.Infect_ID], this.skill_name[human.attribute.skill - 1]), "");
		}

