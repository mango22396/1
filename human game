					num++;
				}
			}
			int num2 = 0;
			if (num == Human.all.Count)
			{
				num2 = 2;
			}
			else if (time >= 11)
			{
				if (time > settime)
				{
					num2 = 1;
				}
				else if (NetGame.instance.local.players.Count > 1 && num <= 1)
				{
					num2 = 1;
				}
				else if (NetGame.instance.local.players.Count == 1 && num == 0)
				{
					num2 = 1;
				}
			}
			if (num2 == 0)
			{
				return;
			}
			Host_Func.gameend = true;
			if (num2 == 1)
			{
				this.Custom_Message(0u, "<size=40><#FFFFFF>游戏结束，<#6de8f4>生存者胜利</color> !</color></size>", "");
			}
			else if (num2 == 2)
			{
				this.Custom_Message(0u, "<size=40><#FFFFFF>游戏结束，<#ac66ed>感染者胜利</color></color></size>", "");
			}
			this.Custom_Message(0u, "<size=20><#FFFFFF>请房主输入 /感染 重置游戏</color></size>", "");
			NetChat.Print("<size=20><#FFFFFF>或点击屏幕右边的结束按钮来重置游戏</color></size>");
		}

