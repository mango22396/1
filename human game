		// Token: 0x06005078 RID: 20600 RVA: 0x0013DC28 File Offset: 0x0013BE28
		public void Fall_Sleep()
		{
			for (int i = 0; i < this.Sleep_human.Count; i++)
			{
				Human human = this.Sleep_human[i];
				try
				{
					float cameraPitch = 100f;
					this.sleep_cameraYaw += 1f;
					human.player.controls.HandleInput(human.player.walkForward, human.player.walkRight, cameraPitch, this.sleep_cameraYaw, human.player.leftExtend, human.player.rightExtend, human.player.jump, human.player.playDead, human.player.holding, human.player.shooting);
					if (human.player.playDead)
					{
						human.player.be_controled = false;
						this.Sleep_human.Remove(human);
					}
				}
				catch
				{
					this.Sleep_human.Remove(human);
				}
			}
		}

