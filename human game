		// Token: 0x0600507A RID: 20602 RVA: 0x0013DE3C File Offset: 0x0013C03C
		private void Float_Up()
		{
			foreach (Human human in this.Float_Human)
			{
				human.ReleaseGrab(0.5f);
				Rigidbody[] rigidbodies = human.rigidbodies;
				for (int i = 0; i < rigidbodies.Length; i++)
				{
					rigidbodies[i].AddForce(60f * Vector3.up.normalized, ForceMode.Force);
				}
			}
		}

		// Token: 0x0600507B RID: 20603 RVA: 0x0013DEC8 File Offset: 0x0013C0C8
		private void Float_Up_Mode()
		{
			Host_Func.Floatup_Mode = !Host_Func.Floatup_Mode;
			if (Host_Func.Floatup_Mode)
			{
				foreach (Human item in Human.all)
				{
					this.Float_Human.Add(item);
				}
				this.preUpdate += this.Float_Up;
				return;
			}
			this.Float_Human.Clear();
			this.preUpdate -= this.Float_Up;
		}

