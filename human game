			{
				try
				{
					if (this.Que_human[j].state == HumanState.Jump)
					{
						this.Que_human[j].state = HumanState.Idle;
					}
				}
				catch
				{
					this.Que_human.Remove(this.Que_human[j]);
				}
			}
			for (int k = 0; k < this.Dance_Humen.Count; k++)
			{
				Human human2 = this.Dance_Humen[k];
				try
				{
					if (human2.attribute.dance)
					{
						human2.ReleaseGrab(5f);
						float leftExtend = 1f;
						float rightExtend = 1f;
						float cameraPitch;
						if (!this.camPitch_mode)
						{
							cameraPitch = -40f + (float)this.dance_i * 80f / 10f;
						}
						else
						{
							cameraPitch = 40f - (float)this.dance_i * 80f / 10f;
						}
						human2.player.controls.HandleInput(human2.player.walkForward, human2.player.walkRight, cameraPitch, human2.player.cameraYaw, leftExtend, rightExtend, human2.player.jump, human2.player.playDead, human2.player.holding, human2.player.shooting);
					}
				}
				catch
				{
					this.Dance_Humen.Remove(human2);
				}
			}
