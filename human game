			if (islocal)
			{
				netPlayer.isLocalPlayer = true;
				netPlayer.localCoopIndex = 1u;
				netPlayer.skin = WorkshopRepository.instance.presetRepo.GetPlayerSkin(1);
				App.StartPlaytimeForItem(netPlayer.skin.workshopId);
				netPlayer.ApplyPreset(netPlayer.skin, true);
			}
			NetGame.instance.players.Add(netPlayer);
			netPlayer.StartNetwork(true);
			if (Host_Func.SIMPLYMOD<><><><><><><><><><><><><><><><><><><><><><><><><><><><><><><><>6X6N6N6664PT2FVR82N6LZ6V24RLL0PH0J6F4J6L22R6T8L0PND6R6D0N86ZT826ZFB0X2V8V848FL04D42628NJ2JPRZ4PFTPXLH828P40P262ZXP6RT646PP8L0BZB00868RL2NX6PL8L6828H848Z20TP0088DZ8VFVL404BT06XR0HTFHPD62ZJZV6X6ZP8JTN828A08EC30FB(Game.currentLevel, null))
			{
				Game.instance.Respawn(netPlayer.human, Vector3.zero);
