		// Token: 0x0600506E RID: 20590 RVA: 0x0013CF5C File Offset: 0x0013B15C
		public void RemoveServerPlayer(NetPlayer player)
		{
			ReplayRecorder.Abort();
			if (NetGame.isServer)
			{
				NetStream netStream = NetGame.BeginMessage(NetMsgId.RemovePlayer);
				try
				{
					netStream.WriteNetId(player.netId);
					for (int i = 0; i < NetGame.instance.readyclients.Count; i++)
					{
						NetGame.instance.SendReliable(NetGame.instance.readyclients[i], netStream);
					}
				}
				finally
				{
					if (netStream != null)
					{
						netStream = netStream.Release();
					}
				}
			}
			NetGame.instance.players.Remove(player);
			player.DespawnPlayer();
