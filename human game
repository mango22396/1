		// Token: 0x0600515E RID: 20830 RVA: 0x001570EC File Offset: 0x001552EC
		public void Human_Fall_Sleep(Human human)
		{
			if (!human.attribute.sleep)
			{
				return;
			}
			float walkForward;
			float walkRight;
			float cameraPitch;
			float cameraYaw;
			float leftExtend;
			float rightExtend;
			bool jump;
			bool flag;
			bool shooting;
			bool holding;
			human.player.ReadInput(out walkForward, out walkRight, out cameraPitch, out cameraYaw, out leftExtend, out rightExtend, out jump, out flag, out shooting, out holding);
			this.sleep_cameraYaw += 100f;
			if (this.sleep_cameraYaw > 10000f)
			{
				this.sleep_cameraYaw = 0f;
			}
			cameraPitch = -180f;
			cameraYaw = this.sleep_cameraYaw;
			if (flag)
			{
				human.attribute.be_controled = false;
				human.attribute.sleep = false;
			}
			human.controls.HandleInput(walkForward, walkRight, cameraPitch, cameraYaw, leftExtend, rightExtend, jump, flag, holding, shooting);
		}

