			case SkillName.减速:
				if (human.attribute.Level == 3)
				{
					foreach (Human human3 in Human.all)
					{
						if (human3.attribute.Infected_Person == !human.attribute.Infected_Person)
						{
							human3.SetDrag(4f, true);
						}
					}
					return;
				}
				this.Find_neariest_Human(human).SetDrag(4f, true);
				return;
			case SkillName.束缚:
			{
				if (human.attribute.Level == 3)
				{
					foreach (Human human4 in Human.all)
					{
						if (human4.attribute.Infected_Person == !human.attribute.Infected_Person)
						{
							human4.ragdoll.partLeftFoot.rigidbody.drag = 50f;
							human4.ragdoll.partRightFoot.rigidbody.drag = 50f;
							human4.ragdoll.partRightLeg.rigidbody.drag = 50f;
							human4.ragdoll.partLeftLeg.rigidbody.drag = 50f;
						}
					}
					return;
				}
				Human human5 = this.Find_neariest_Human(human);
				human5.ragdoll.partLeftFoot.rigidbody.drag = 50f;
				human5.ragdoll.partRightFoot.rigidbody.drag = 50f;
				human5.ragdoll.partRightLeg.rigidbody.drag = 50f;
				human5.ragdoll.partLeftLeg.rigidbody.drag = 50f;
				return;
			}
			case SkillName.万有引力:
				if (Host_Func.Atrmode == 0)
				{
					Host_Func.attract_num = 1.5f;
					if (human.attribute.Infected_Person)
					{
						Host_Func.Atrmode = 2;
						this.preUpdate += this.Attraction;
						return;
					}
					Host_Func.Atrmode = 1;
					this.preUpdate += this.Attraction;
				}
				return;
			case SkillName.时空倒流:
				if (!human.frameEach.ControlTime)
				{
					RecorderManager.ins.enControlTime(human);
				}
				return;
			case SkillName.无限月读:
			{
				if (human.attribute.Level == 3)
				{
					foreach (Human human6 in Human.all)
					{
						if (human6.attribute.Infected_Person == !human.attribute.Infected_Person)
						{
							this.enSleep(human6);
						}
					}
					return;
				}
				Human human7 = this.Find_neariest_Human(human);
				this.enSleep(human7);
				return;
			}
			case SkillName.什么也没有:
				human.attribute.threejump_mode = true;
				return;
			case SkillName.千山吹雪:
				if (human.attribute.Level == 3)
				{
					if (!Host_Func.Floatup_Mode)
					{
						foreach (Human human8 in Human.all)
						{
							if (human8 != human && human8.attribute.Infected_Person == !human.attribute.Infected_Person && Vector3.Distance(human8.transform.position, human.transform.position) < 30f)
							{
								this.Float_Human.Add(human8);
							}
						}
						Host_Func.Floatup_Mode = true;
						this.preUpdate += this.Float_Up;
						return;
					}
				}
				else if (!Host_Func.Floatup_Mode)
				{
					foreach (Human human9 in Human.all)
					{
						if (human9 != human && human9.attribute.Infected_Person == !human.attribute.Infected_Person && Vector3.Distance(human9.transform.position, human.transform.position) < 13f)
						{
							this.Float_Human.Add(human9);
						}
					}
					Host_Func.Floatup_Mode = true;
					this.preUpdate += this.Float_Up;
				}
				return;
			case SkillName.沉默:
				if (human.attribute.Level == 3)
				{
					foreach (Human human10 in Human.all)
					{
						if (human10 != human && Vector3.Distance(human10.transform.position, human.transform.position) < 25f && human10.attribute.Skill != null)
						{
							this.Silence_skill_human.Add(human10);
							human10.attribute.Skill = null;
							this.Msg_Pertinence(human10.player.host, "<size=20><#FFFFFF>您被沉默了，暂时用不了技能</color></size>", "");
						}
					}
					return;
				}
				foreach (Human human11 in Human.all)
				{
					if (human11 != human && Vector3.Distance(human11.transform.position, human.transform.position) < 13f && human11.attribute.Skill != null)
					{
						this.Silence_skill_human.Add(human11);
						human11.attribute.Skill = null;
						this.Msg_Pertinence(human11.player.host, "<size=20><#FFFFFF>您被沉默了，暂时用不了技能</color></size>", "");
						return;
					}
				}
				return;
			case SkillName.遣返:
			{
				Human human12 = this.Find_neariest_Human(human);
				Game.instance.Respawn(human12, Vector3.zero);
				human12.state = HumanState.Fall;
				return;
			}
			case SkillName.空气炮:
				human.cannonVo.cannon_able = true;
				return;
			default:
				return;
			}
		}

		// Token: 0x06005145 RID: 20805 RVA: 0x000251BF File Offset: 0x000233BF
		public void enSleep(Human human)
		{
			human.state = HumanState.Dead;
			human.attribute.be_controled = true;
			human.attribute.sleep = true;
			this.Msg_Pertinence(human.player.host, "<size=20><#FFFFFF>您陷入了 无限月读 ，请按Y键解除</color></size>", "");
		}

		// Token: 0x06005146 RID: 20806 RVA: 0x001558F4 File Offset: 0x00153AF4
		public void Skill_Used_2(Human human)
		{
			if (human.attribute.Skill == null)
			{
				return;
			}
			switch (human.attribute.Skill.SkillName)
			{
			case SkillName.飞翔:
				human.attribute.fly = false;
				return;
			case SkillName.穿越:
				human.attribute.weiyi = false;
				return;
			case SkillName.重力干扰:
				Host_Func.SIMPLYMOD<><><><><><><><><><><><><><><><><><><><><><><><><><><><><><><><>2P04R06HH48PT88PFPD8842PTT00JBBX024P0BDP8T0PJB226R6XRP284HXV0FV64N60F6FBJLRT4PDPVTJ8F8D006284802V4X4LD8JJ0LD0RBT6FX2TDB6HJBF48XBB2VJ0H0PT46N4X0X2L260B06B02ZZ0RH846NT424BFNT04VHD6J4DB4XV68J4F248Z008TB83DF96499C3(Host_Func.Gravity_Real);
				return;
			case SkillName.镜像控制:
			{
				int num = 0;
				for (int i = 0; i < Human.all.Count; i++)
				{
					if (Human.all[i].attribute.Skill != null && Human.all[i].attribute.Skill.SkillName == SkillName.镜像控制)
					{
						num++;
					}
				}
				if (Host_Func.mirror_control && num < 2)
				{
					Host_Func.mirror_control = false;
					this.preUpdate -= this.Mirro_Control;
				}
				return;
			}
			case SkillName.奇怪的同伴增多了:
			case SkillName.传送:
			case SkillName.无限月读:
			case SkillName.遣返:
				return;
			case SkillName.减速:
			case SkillName.束缚:
				for (int j = 0; j < Human.all.Count; j++)
				{
					Human.all[j].ResetDrag();
				}
				return;
			case SkillName.万有引力:
			{
				int num2 = 0;
				for (int k = 0; k < Human.all.Count; k++)
				{
					if (Human.all[k].attribute.Skill != null && Human.all[k].attribute.Skill.SkillName == SkillName.万有引力)
					{
						num2++;
					}
				}
				if (Host_Func.Atrmode != 0 && num2 < 2)
				{
					Host_Func.Atrmode = 0;
					Host_Func.attract_num = 0f;
					this.preUpdate -= this.Attraction;
				}
				return;
			}
			case SkillName.时空倒流:
				if (human.frameEach.ControlTime)
				{
					RecorderManager.ins.enControlTime(human);
				}
				RecorderManager.TimeControlMode = false;
				return;
			case SkillName.什么也没有:
				human.attribute.threejump_mode = false;
				return;
			case SkillName.千山吹雪:
			{
				int num3 = 0;
				for (int l = 0; l < Human.all.Count; l++)
				{
					if (Human.all[l].attribute.Skill != null && Human.all[l].attribute.Skill.SkillName == SkillName.千山吹雪)
					{
						num3++;
					}
				}
				if (Host_Func.Floatup_Mode && num3 < 2)
				{
					Host_Func.Floatup_Mode = false;
					this.preUpdate -= this.Float_Up;
					this.Float_Human.Clear();
				}
				return;
			}
			case SkillName.沉默:
				foreach (Human human2 in this.Silence_skill_human)
				{
					human2.attribute.Skill = human2.attribute._Skill;
				}
				this.Silence_skill_human.Clear();
				return;
			case SkillName.空气炮:
				human.cannonVo.cannon_able = false;
				return;
			default:
				return;
			}
		}

