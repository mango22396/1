		// Token: 0x0600516F RID: 20847 RVA: 0x00157F28 File Offset: 0x00156128
		public void quanxian(NetHost netHost)
		{
			this.touquanxian = !this.touquanxian;
			if (this.touquanxian)
			{
				NetStream netStream = NetGame.BeginMessage(NetMsgId.AddHost);
				try
				{
					netStream.Write(netHost.players[0].human.attribute.chat_command);
					for (int i = 0; i < NetGame.instance.readyclients.Count; i++)
					{
						if (NetGame.instance.readyclients[i] != netHost)
						{
							NetGame.instance.SendReliable(NetGame.instance.readyclients[i], netStream);
						}
					}
				}
				finally
				{
					if (netStream != null)
					{
						netStream = netStream.Release();
					}
				}
			}
		}

