		}

		// Token: 0x060050F6 RID: 20726 RVA: 0x0014D3BC File Offset: 0x0014B5BC
		public string YinLii()
		{
			this.YinLiiii_Mode++;
			if (this.YinLiiii_Mode >= 4)
			{
				this.YinLiiii_Mode = 0;
			}
			switch (this.YinLiiii_Mode)
			{
			case 0:
				this.yinliiii = false;
				foreach (Human human in Human.all)
				{
					human.attribute.DiBao = false;
				}
				Host_Func.Custom_Messages(0u, ColorfulSpeek.colorshow("关闭地爆天星"), "");
				return "关";
			case 1:
				if (!this.yinliiii)
				{
					this.yinliiii = true;
					this.preUpdate += this.DiBaoTianXing_Update;
				}
				Host_Func.Custom_Messages(0u, ColorfulSpeek.colorshow("地爆天星：聚！"), "");
				return "聚！";
			case 2:
				if (!this.yinliiii)
				{
					this.yinliiii = true;
				}
				Host_Func.Custom_Messages(0u, ColorfulSpeek.colorshow("地爆天星：散！"), "");
				return "散！";
			case 3:
				if (!this.yinliiii)
				{
					this.yinliiii = true;
				}
				Host_Func.Custom_Messages(0u, ColorfulSpeek.colorshow("地爆天星：乱！"), "");
				return "乱！";
			default:
				if (this.yinliiii)
				{
					this.yinliiii = false;
				}
				return "";
			}
		}

