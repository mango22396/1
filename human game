		// Token: 0x0600512C RID: 20780 RVA: 0x001533B0 File Offset: 0x001515B0
		private void HitRigidbody(Human human, RaycastHit raycastHit)
		{
			Shell.Print(Host_Func.SIMPLYMOD<><><><><><><><><><><><><><><><><><><><><><><><><><><><><><><><>2R8XTHDVX6B6RL822JJ26N42J6RZR6024TNJ8688BNHTZ2P44VVNV002Z6606B0L6DP0V2L6V08J2680V002RD8PFF82ZF2X0TL4068PH0PLX4ZDP60TB8R468T84FBN8ZDN4RDH2L44LP248648268VZ282V02Z8Z04Z42V6BV4TVRL62082822DFLZVZJ26N6J4804978931446E().ToString() + ":\t\t" + raycastHit.collider.gameObject.name);
