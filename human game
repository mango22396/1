		public void RestartCheckpointWithoutPlayer(int checkpointNumber, int subObjectives)
		{
			if (Game.currentLevel.respawnLocked)
			{
				return;
			}
			Game.instance.currentCheckpointNumber = Mathf.Min(checkpointNumber, Game.currentLevel.checkpoints.Length - 1);
			Game.instance.currentCheckpointSubObjectives = subObjectives;
			if (Game.currentLevel.prerespawn != null)
			{
				Game.currentLevel.prerespawn(checkpointNumber, false);
			}
			Host_Func.SIMPLYMOD<><><><><><><><><><><><><><><><><><><><><><><><><><><><><><><><>86LP64R84V266J6TRD0P8444R4VVX242PNN08P0440DL6VL0VLX484X4F4VLP06P0FPB660Z0T2XVVZ6P6XJP0PLN8DT88R4BL8ND84R4VJJL6XH6Z46ZJ8068L826F2Z8THP62F2DH2FD8NVTHV4RB4LP0ZZD08N6482N8024VTZ6J00V0FD088PB4XP6666P40TFJ216900B0DDE(Game.currentLevel, Game.instance.currentCheckpointNumber, Game.instance.currentCheckpointSubObjectives);
			if (NetGame.isServer)
			{
				NetSceneManager.ResetLevel(Game.instance.currentCheckpointNumber, Game.instance.currentCheckpointSubObjectives);
			}
			Host_Func.SIMPLYMOD<><><><><><><><><><><><><><><><><><><><><><><><><><><><><><><><>HB48806H4VTBH6ZTJPHD4X82NVF28T06F42BBD88XJ2TDDTP6662P2886FD48TFHFTT4DV2D0R2P4P0866FJJRL064LT64F66R40H8J8V8PV04LB68DPLTVLF6JJ4ZD6JZLF6068H280ZN020H4H6888T8200X426L446D462420FT006P4DV40TJ40886888P4086BDDDF68CCA70(Game.currentLevel);
